{
  "name": "ydh-sider-obfuscation-service",
  "version": "1.2.0",
  "description": "NodeJS Express Service to obfuscate URL query parameters before sending to SIDeR eSP.",
  "keywords": [
    "YDH",
    "NHS",
    "obfuscation"
  ],
  "main": "src/app.js",
  "scripts": {
    "test": "npm run lint",
    "start": "node src/app.js",
    "nodemon": "nodemon src/app.js",
    "lint": "eslint src/*.js",
    "install-windows-service": "winser -i -s -c -a -n \"SIDeR Contextual Link Obfuscation Service\"",
    "uninstall-windows-service": "winser -r -x -s -n \"SIDeR Contextual Link Obfuscation Service\""
  },
  "author": {
    "name": "Frazer Smith",
    "email": "frazer.smith@ydh.nhs.uk"
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "^5.16.0",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "obfuscated-querystring": "git+https://github.com/BlackPearSw/obfuscated-querystring.git",
    "winser": "^1.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Somerset-SIDeR-Programme/ydh-sider-obfuscation-service.git"
  },
  "bugs": {
